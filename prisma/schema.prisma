// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Food {
  id   Int    @id @default(autoincrement())
  name String
}

model Users {
  id            Int       @id @default(autoincrement())
  login_id      String
  password      String
  qos           String
  is_encrypt    Boolean
  last_login_at DateTime
  created_at    DateTime
  updated_at    DateTime
  deleted_at    DateTime
}

